{
  "name": "viewstestparcel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@viewstools/create-react-app": "^24.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "concurrently --kill-others npm:start:*",
    "build": "views-morph src --as react-dom && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "start:react": "react-app-rewired start",
    "start:views": "views-morph src --watch --as react-dom",
    "views:clean": "views-morph src --clean"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ios > 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
